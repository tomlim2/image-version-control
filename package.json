{
  "name": "pixtree-workspace",
  "version": "1.0.0",
  "description": "Pixtree - AI image version control system like Git for your creative workflow",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean"
  },
  "keywords": ["ai", "image", "version-control", "cli", "creative-tools", "pixtree"],
  "author": "younsoolim",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.7.5",
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.13",
    "eslint": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}